{
	"name": "java-method-parser",
	"version": "0.4.5",
	"description": "A node utility to get the methods out of java files",
	"main": "index.js",
	"repository": "danielmschmidt/java-method-parser",
	"keywords": ["java", "parser", "methods"],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DanielMSchmidt/java-method-parser/issues"
	},
	"homepage": "https://github.com/DanielMSchmidt/java-method-parser#readme",
	"scripts": {
		"precommit": "lint-staged",
		"test": "jest",
		"fmt": "prettier --write **/*.{js,json,css,md}"
	},
	"lint-staged": {
		"*.{js,json,css,md}": ["prettier --write", "git add"]
	},
	"author": {
		"name": "Daniel Schmidt",
		"email": "danielmschmidt92@gmail.com",
		"url": "danielmschmidt.de"
	},
	"engines": {
		"node": ">=4"
	},
	"jest": {
		"collectCoverage": true,
		"coverageThreshold": {
			"global": {
				"statements": 100,
				"branches": 90,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"files": ["index.js", "README.md"],
	"devDependencies": {
		"coveralls": "^3.0.0",
		"husky": "0.14.3",
		"jest": "^22.4.0",
		"lint-staged": "7.0.5",
		"nyc": "^11.4.1",
		"prettier": "1.12.1"
	},
	"nyc": {
		"reporter": ["lcov", "text"]
	}
}
